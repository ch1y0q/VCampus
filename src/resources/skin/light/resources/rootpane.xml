<skin xmlns="http://weblookandfeel.com/XmlSkin">

    <!-- com.alee.laf.rootpane.WebRootPane -->

    <!-- Root pane -->
    <style type="rootpane">
        <component>
            <background>237,237,237</background>
            <foreground>black</foreground>
        </component>
        <ui>
            <installComponents>false</installComponents>
            <iconSize>16</iconSize>
            <emptyTitleText>weblaf.rootpane.empty.title</emptyTitleText>
            <setupButtonIcons>true</setupButtonIcons>
            <displayTitleComponent>true</displayTitleComponent>
            <displayWindowButtons>true</displayWindowButtons>
            <displayMinimizeButton>true</displayMinimizeButton>
            <displayMaximizeButton>true</displayMaximizeButton>
            <displayCloseButton>true</displayCloseButton>
            <displayMenuBar>true</displayMenuBar>
        </ui>
        <painter />

        <!-- Content pane -->
        <style type="panel" id="content" />

    </style>

</skin>